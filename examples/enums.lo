enum State { None | Loading | Loaded }

main {
    let state: State = #None;
    State_print(state);
    out("Press enter to load...");

    let input: str = "";
    in(&input);

    state = #Loading;
    State_print(state);
    wait_s(3);

    state = #Loaded;
    State_print(state);
}

func State_print(state: State) match state {
    #None -> out("None")
    #Loading -> out("Loading")
    #Loaded -> out("Loaded")
}
