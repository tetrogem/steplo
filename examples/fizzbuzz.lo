main {
    out("Enter how many iterations to perform:");

    let iters: uint = <<uint>>in();

    let i: uint = 1;
    while (i <= iters) {
        if ((i % 15) == 0) out("FizzBuzz")
        else if ((i % 3) == 0) out("Fizz")
        else if ((i % 5) == 0) out("Buzz")
        else out(i)

        i = (i + 1);
    }

    out("Done!");
}

