main {
    out("Enter how many iterations to perform:");

    let iters: uint = 0;
    in(<<&str>>&iters);

    let i: uint = 1;
    while (i <= iters) {
        let result: val = fizzbuzz(i);
        out(result);
        i = (i + 1);
    }

    out("Done!");
}

func fizzbuzz(num: uint) -> val
    if ((num % 15) == 0) "FizzBuzz"
    else if ((num % 3) == 0) "Fizz"
    else if ((num % 5) == 0) "Buzz"
    else num
