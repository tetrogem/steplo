static foo: str = "Static!";

main {
    out(foo);

    let foo: str = "Local!";
    out(foo);
    foo = "Local (Mutated)";
    out(foo);

    print_static();
    mutate_static("Static (Mutated)");
    print_static();

    out(foo);
}

fn print_static() out(foo)
fn mutate_static(val: str) foo = val

// Expected stdout:
// Static!
// Local!
// Local (Mutated)
// Static!
// Static (Mutated)
// Local (Mutated)
