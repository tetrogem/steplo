main {
    out("1");
    out("2");
    out("3");

    in();

    stdout_clear();
    out("4");
    out("5");
    out("6");

    let line: str = stdout_read(2);
    line = (line ~ "Hello!");
    stdout_write(line, 2);
    stdout_write(line, 10);

    out(("stdout line count: " ~ stdout_len()));
}

