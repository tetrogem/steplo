main {
    let a: num = 100;
    let b: num = 987;

    out(("A: " ~ a));
    out(("B: " ~ b));

    swap(&a, &b);

    out(("A: " ~ a));
    out(("B: " ~ b));
}

func swap(a: &num, b: &num) {
    let temp: num = *a;
    *a = *b;
    *b = temp;
}

// Expected stdout:
// A: 100
// B: 987
// A: 987
// B: 100
