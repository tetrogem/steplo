main |a, b, c| {
    a = "5";
    b = "5";
    c = "12";

    if a | eq b {
        out("A is equal to B...");

        if a | eq c {
            out("...and equal to C!");
        } else {
            out("...but not equal to C...");
        }
    } else {
        out("A is not equal to B...");

        if a | eq c {
            out("...but it is equal to C!");
        } else {
            out("...or C...");
        }
    }

    out("Done!");
}
